{
	"index":"index:ROOT",
	"tpl":{
		"main":"/-user/admin/main.html.js",
		"list":"/-user/admin/list.html.js",
		"settings":"/-user/admin/settings.html.js",
		"index":"/-sitemap/index.html.js",
		"controller":"/-controller/layout.html.js"
	},
	"layout":{
		"index:ROOT":{
			"BODY":"main:ROOT",
			"FOOTER":"main:FOOTER",
			"HEADER":"main:HEADER"
		},
		"settings:ROOT":{
			"DATABASE":"settings:DATABASE"
		}
	},
	"jsontpl":{
		"index:ROOT":"/-sitemap/get-head?path=${env.bread.path}&root=${env.bread.root}"
	},
	"json":{
		"main:ROOT":"/-user/admin/get-check",
		"list:ROOT":"/-user/admin/get-list",
		"settings:DATABASE":"/-user/admin/get-settings"
	},
	"childs":{
		"settings":{
			"head":{ "title":"Настройки" },
			"layout":{
				"index:ROOT":{
					"BODY":"settings:ROOT"
				}
			}
		},
		"list":{
			"head":{ "title":"Список пользователей" },
			"layout":{
				"index:ROOT":{
					"BODY":"list:ROOT"
				}
			}
		},
		"404":{
			"head":{ "hidden":true, "title":"404 - Страница не найдена" },
			"layout":{
				"index:ROOT":{
					"BODY":"controller:ER404"
				}
			}
		}
	}
}