{
	"index":"index:ROOT",
	
	"tpl":{
		"main":"/-user/main.html.js",
		"signup":"/-user/signup.html.js",
		"signin":"/-user/signin.html.js",
		"logout":"/-user/logout.html.js",
		"result":"/-user/result.html.js",
		"index":"/-sitemap/index.html.js",
		"controller":"/-controller/layout.html.js",
		"sitemap":"/-sitemap/sitemap.html.js"
	},
	"layout":{
		"index:ROOT":{
			"BODY":"main:ROOT",
			"FOOTER":"main:FOOTER",
			"HEADER":"main:HEADER"
		}
	},
	"jsontpl":{
		"index:ROOT":"/-sitemap/get-head?path=${env.bread.path}&root=${env.bread.root}"
	},
	"json":{
		"main:ROOT":"/-user/get-user",
		"signup:ROOT":"/-user/get-user",
		"signin:ROOT":"/-user/get-user",
		"logout:ROOT":"/-user/get-user",
		"sitemap:ROOT":"/-sitemap/get-data"
	},
	"css2":{
		"float-label":"/-float-label/style.css"
	},
	"childs":{
		"signup":{
			"head":{ "title":"Регистрация", "css2":["float-label"] },
			"layout":{
				"index:ROOT":{
					"BODY":"signup:ROOT"
				}
			}
		},
		"signin":{
			"head":{ "title":"Вход", "css2":["float-label"] },
			"layout":{
				"index:ROOT":{
					"BODY":"signin:ROOT"
				}
			}
		},
		"result":{
			"head":{ "title":"Подтверждение" },
			"layout":{
				"index:ROOT":{
					"BODY":"result:ROOT"
				}
			}
		},
		"logout":{
			"head":{ "title":"Вход" },
			"layout":{
				"index:ROOT":{
					"BODY":"logout:ROOT"
				}
			}
		},
		"sitemap":{
			"head":{
				"hidden":true,
				"title":"Карта сайта"
			},
			"layout":{
				"index:ROOT":{
					"BODY":"sitemap:ROOT"
				}
			}
		},
		"404":{
			"head":{ "hidden":true, "title":"404 - Страница не найдена" },
			"layout":{
				"index:ROOT":{
					"BODY":"controller:ER404"
				}
			}
		}
	},
	"head":{
		"title":"Личный кабинет пользователя",
		"description": "",
		"image_src": ""
	}
}